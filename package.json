  "scripts": {
    "bootstrap:build": "node ./bootstrap/build.js",
    "bootstrap:install": "node ./bootstrap/install.js",
    "bootstrap": "yarn run bootstrap:install && yarn run bootstrap:build",
    "parcel:serve": "parcel serve src/pages/*.html --no-source-maps",
    "parcel:build": "parcel build src/pages/*.html --no-source-maps",
    "clean": "rm -rf dist .parcel-cache",
    "dev": "npm-run-all -s clean -p parcel:serve",
    "build": "yarn bootstrap && yarn parcel:build"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.1.1",
    "@parcel/transformer-sass": "^2.1.1",
    "@parcel/transformer-typescript-types": "^2.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@ut-essentials/posthtml-extends": "https://gitpkg.now.sh/IamStephan/ut-essentials/packages/posthtml-extends?master",
    "@ut-essentials/posthtml-pipeline": "https://gitpkg.now.sh/IamStephan/ut-essentials/packages/posthtml-pipeline?master",
    "@ut-essentials/posthtml-tokens": "https://gitpkg.now.sh/IamStephan/ut-essentials/packages/posthtml-tokens?master",
    "@ut-essentials/tailwindcss-preset-screens": "https://gitpkg.now.sh/IamStephan/ut-essentials/packages/tailwindcss-preset-screens?master",
    "@ut-essentials/tailwindcss-preset-themable": "https://gitpkg.now.sh/IamStephan/ut-essentials/packages/tailwindcss-preset-themable?master",
    "autoprefixer": "^10.4.1",
    "color": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.1.1",
    "postcss": "^8.4.5",
    "posthtml-expressions": "^1.9.0",
    "posthtml-extend": "^0.6.2",
    "posthtml-extends": "link:./lib/posthtml-extends",
    "posthtml-pipeline": "link:./lib/posthtml-pipeline",
    "posthtml-tokens": "link:./lib/posthtml-tokens",
    "posthtml-widgets": "link:./lib/posthtml-widgets",
    "tailwindcss": "^3.0.8",
    "typescript": ">=3.0.0"
  }
}
